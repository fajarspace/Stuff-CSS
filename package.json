{
  "name": "stuffcss",
  "version": "1.1.0",
  "description": "Minimal CSS Framework for semantic HTML",
  "author": "fajarspace",
  "main": "index.js",
  "homepage": "https://stuffcss.netlify.app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fajarspace/stuff-css.git"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "watch": "npm-run-all --parallel watch:stuff watch:docs:scss watch:docs:html watch:dist:js --silent",
    "builds": "npm-run-all scss:sorter:stuff build:stuff scss:sorter:docs build:docs:css build:dist:js --silent",
    "build:docs": "npm-run-all --parallel build:docs:css build:docs:js build:docs:html --silent",
    "build:docs:css": "npm-run-all css:compile:docs css:prefix:docs css:minify:docs --silent",
    "build:docs:html": "npm-run-all htmlincludes:docs --silent",
    "build:dist:js": "npm-run-all js:bundle:dist:stuff js:bundle:dist:stuffcustomization js:babel:dist:stuff js:babel:dist:stuffcustomization js:babel:dist:stuffgrid js:babel:dist:stuffmodal js:minify:dist:stuff js:minify:dist:stuffcustomization js:minify:dist:stuffgrid js:minify:dist:stuffmodal --silent",
    "build:stuff": "npm-run-all css:compile:stuff css:prefix:stuff css:minify:stuff --silent",
    "css:compile:docs": "sass --style expanded --source-map --embed-sources --no-error-css src/static/scss/:src/static/css/",
    "css:compile:stuff": "sass --style expanded --source-map --embed-sources --no-error-css scss/:dist/css/",
    "css:minify:docs": "cleancss -O1 --with-rebase --source-map --source-map-inline-sources --batch --batch-suffix .min src/static/css/*.css !src/static/css/*.min.css",
    "css:minify:stuff": "cleancss -O1 --with-rebase --source-map --source-map-inline-sources --batch --batch-suffix .min dist/css/*.css dist/css/*/*.css !dist/css/*.min.css !dist/css/*/*.min.css",
    "css:prefix:docs": "postcss --config src/static/css --replace src/static/css/*.css !src/static/css/*.min.css",
    "css:prefix:stuff": "postcss --config dist/css --replace dist/css/*.css dist/css/*/*.css !dist/css/*.min.css",
    "htmlincludes:docs": "html-includes --src docs/src/pages --dest docs --minify minifyJS=true --quiet",
    "js:babel:dist:stuff": "babel dist/js/stuff.min.js --out-dir dist/js/ --presets=@babel/preset-env --quiet",
    "js:babel:dist:stuffcustomization": "babel dist/js/stuffcustomization.min.js --out-dir dist/js/ --presets=@babel/preset-env --quiet",
    "js:babel:dist:stuffgrid": "babel dist/js/stuffgrid.js --out-file dist/js/stuffgrid.min.js --presets=@babel/preset-env --quiet",
    "js:babel:dist:stuffmodal": "babel dist/js/stuffmodal.js --out-file dist/js/stuffmodal.min.js --presets=@babel/preset-env --quiet",
    "js:bundle:dist:stuff": "rollup dist/js/stuff.js --file dist/js/stuff.min.js --format iife --silent",
    "js:bundle:dist:stuffcustomization": "rollup dist/js/stuffcustomization.js --file dist/js/stuffcustomization.min.js --format iife --silent",
    "js:minify:dist:stuff": "uglifyjs dist/js/stuff.min.js --compress --mangle --output dist/js/stuff.min.js",
    "js:minify:dist:stuffcustomization": "uglifyjs dist/js/stuffcustomization.min.js --compress --mangle --output dist/js/stuffcustomization.min.js",
    "js:minify:dist:stuffgrid": "uglifyjs dist/js/stuffgrid.min.js --compress --mangle --output dist/js/stuffgrid.min.js",
    "js:minify:dist:stuffmodal": "uglifyjs dist/js/stuffmodal.min.js --compress --mangle --output dist/js/stuffmodal.min.js",
    "scss:sorter:docs": "postcss --config scss ./scss/**/*.scss --replace",
    "scss:sorter:stuff": "postcss --config scss ./scss/**/*.scss --replace",
    "prebuild:docs:html": "echo '[stuffcss] Create documentation pages'",
    "prebuild:dist:js": "echo '[stuffcss] Bundle and minify dist .js files'",
    "precss:compile:docs": "echo '[stuffcss] Compile documentation .css file'",
    "precss:compile:stuff": "echo '[stuffcss] Compile Stuff .css files'",
    "precss:minify:docs": "echo '[stuffcss] Minify documentation .css file'",
    "precss:minify:stuff": "echo '[stuffcss] Minify Stuff .css files'",
    "prescss:sorter:docs": "echo '[stuffcss] Order documentation .scss properties'",
    "prescss:sorter:stuff": "echo '[stuffcss] Order Stuff .scss properties'",
    "postbuild:docs:css": "echo '\\033[32m[stuffcss] Docs CSS build done\\033[0m'",
    "postbuild:docs:html": "echo '\\033[32m[stuffcss] Documentation pages done\\033[0m'",
    "postbuild:dist:js": "echo '\\033[32m[stuffcss] Dist .js build done\\033[0m'",
    "postbuild:stuff": "echo '\\033[32m[stuffcss] Stuff CSS build done\\033[0m'",
    "watch:docs:html": "nodemon --watch docs/src/pages --ext html --exec 'npm run build:docs:html'",
    "watch:dist:js": "nodemon --watch dist/js/ --ext js --ignore *.min.js --exec 'npm run build:dist:js'",
    "watch:docs:scss": "nodemon --watch src/static/scss/ --ext scss --exec 'npm run build:docs:css'",
    "watch:stuff": "nodemon --watch scss/ --ext scss --exec 'npm run build:stuff'"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@next/font": "13.1.6",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.2.3",
    "clean-css-cli": "^5.6.2",
    "css-declaration-sorter": "^6.3.1",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "html-includes": "^5.0.0",
    "next": "13.1.6",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-scss": "^4.0.6",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "18.2.0",
    "react-safe": "^1.3.0",
    "rollup": "^3.12.0",
    "sass": "^1.57.1",
    "typeit-react": "^2.6.4",
    "typescript": "4.9.5",
    "uglify-js": "^3.17.4"
  }
}
